<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>rzmoz</Authors>
    <Company>rzmoz</Company>
    <Product>DotNet.Basics.SevenZip</Product>
    <Description>7zip SDK for robust archive operations</Description>
    <RepositoryUrl>https://github.com/rzmoz/DotNet.Standard</RepositoryUrl>
    <PackageLicenseUrl>http://www.7-zip.org/license.txt</PackageLicenseUrl>    
    <Version>0.0.0</Version>
    <AssemblyVersion>0.0.0</AssemblyVersion>
    <FileVersion>0.0.0</FileVersion>
  </PropertyGroup>
  <ItemGroup>
    <None Remove="7za.dll" />
    <None Remove="7za.exe" />
    <None Remove="7zxa.dll" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="7za.dll" />
    <EmbeddedResource Include="7za.exe" />
    <EmbeddedResource Include="7zxa.dll" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="System.IO.Compression" Version="4.3.0" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\DotNet.Standard\DotNet.Standard.csproj" />
  </ItemGroup>
</Project>